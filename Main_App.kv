#: import CheckBox kivy.uix.checkbox

<Lock@Button>:
    background_normal: "/Users/Benac2003/Documents/Codding/Smart-Watch/image/loading-1.jpg"
<Home@Button>:
    background_normal: " "

<Homescreen>:
    BoxLayout:
        Lock:
            pos_hint: {"centre_x":1 ,"y": 1 }
            on_press:
                root.manager.transition.direction = "left"
                root.manager.transisition.duration = 1
                root.manager.current =  "Lockscreen"

<Lockscreen>:
    BoxLayout:
        Home:
            pos_hint: {"centre_x":1 ,"y": 1 }
            on_press:
                root.manager.transition.direction = "left"
                root.manager.transisition.duration = 1
                root.manager.current =  "Homescreen"

        

<TimeDate@Label>:
    font_size: 32
    font_colour: 51,153,255
    size: 150, 50
    background_colour: 1,1,1,1 


<CustFloatLayout>:
    TimeDate: 
        text:"fdsf"
        size_hint_x: .80
        pos_hint: {"left":5, "centre_y":5}
